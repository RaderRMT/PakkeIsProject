    TextHoldTime: 2
    TextColor:
      serializedVersion: 2
      rgba: 4294297274
    SizeEffect: 0
    SequencingTypeNext: 0
    Clip: {fileID: 0}
  - Text: "Je ne peux pas purifier seule, une \xE2me errante qui s'est m\xEAl\xE9e
