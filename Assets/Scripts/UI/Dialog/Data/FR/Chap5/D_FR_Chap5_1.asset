    ShowLetterByLetter: 0
    TextHoldTime: 3
    TextColor:
      serializedVersion: 2
      rgba: 4294958549
    SizeEffect: 0
    SequencingTypeNext: 0
    Clip: {fileID: 0}
  - Text: "Je sens que la b\xEAte peut surgir d'un instant \xE0 l'autre. "
