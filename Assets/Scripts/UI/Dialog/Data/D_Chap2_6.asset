    TextColor:
      serializedVersion: 2
      rgba: 4294836198
    SizeEffect: 0
    SequencingTypeNext: 0
    Clip: {fileID: 0}
  - Text: "Je n'ai pas le pouvoir absolu, je guide les animaux mais je ne les contr\xF4les
