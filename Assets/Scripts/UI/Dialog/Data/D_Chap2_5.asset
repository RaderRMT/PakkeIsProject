    TextHoldTime: 4
    TextColor:
      serializedVersion: 2
      rgba: 4294836198
    SizeEffect: 0
    SequencingTypeNext: 0
    Clip: {fileID: 0}
  - Text: "Les \xE2mes des animaux tu\xE9s sans respect ne trouvent pas la paix."
